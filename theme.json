{
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"spacing": {
			"spacingSizes": [
				{
					"name": "Small",
					"size": "clamp(.5rem, 2.5vw, 1rem)",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "clamp(1.5rem, 4vw, 2rem)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(2rem, 5vw, 3rem)",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "clamp(3rem, 7vw, 5rem)",
					"slug": "x-large"
				},
				{
					"name": "2xl",
					"size": "clamp(4rem, 9vw, 7rem)",
					"slug": "xx-large"
				},
				{
					"name": "3xl",
					"size": "clamp(5rem, 12vw, 9rem)",
					"slug": "xxx-large"
				},
				{
					"name": "4xl",
					"size": "clamp(6rem, 14vw, 13rem)",
					"slug": "xxxx-large"
				}
			],
			"units": ["px", "em", "rem", "vh", "vw", "%"],
			"blockGap": true,
			"padding": true,
			"margin": true
		},
		"typography": {
			"dropCap": false,
			"customFontSize": false,
			"fontWeight": false,
			"letterSpacing": false,
			"fontStyle": false,
			"lineHeight": false,
			"textDecoration": false,
			"fontSizes": [
				{
					"slug": "small",
					"size": "16px",
					"name": "Small"
				},
				{
					"slug": "normal",
					"size": "20px",
					"name": "Normal"
				},
				{
					"slug": "large",
					"size": "26px",
					"name": "Large"
				}
			]
		},
		"blocks": {
			"core/button": {
				"border": {
					"radius": false
				}
			},
			"core/pullquote": {
				"border": {
					"color": true,
					"radius": false,
					"style": false,
					"width": false
				}
			}
		}
	}
}
